{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/descargo-de-responsabilidad/?$",
			"dest": "_render"
		},
		{
			"src": "^/politica-de-cookies/?$",
			"dest": "_render"
		},
		{
			"src": "^/politica-de-privacidad/?$",
			"dest": "_render"
		},
		{
			"src": "^/terminos-de-servicio/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}