{
    "functions": {
        "src/pages/api/*.ts": {
            "maxDuration": 10
        }
    },
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, s-maxage=300, stale-while-revalidate=600"
                }
            ]
        }
    ]
}